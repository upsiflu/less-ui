{
  "private": true,
  "type": "module",
  "name": "example",
  "scripts": {
    "postinstall": "elm-tooling install",
    "start-without-documentation": "run-pty % elm-watch hot % npm run esbuild -- --serve=9000 --servedir=public % node dev-server.js 9000",
    "start": "run-pty run-pty.json",
    "try-production": "run-pty % esbuild --serve=9000 --servedir=public % node dev-server.js 9000",
    "build": "elm-watch make --optimize && npm run esbuild -- --minify",
    "esbuild": "esbuild app.ts garden.ts features.ts --bundle --outdir=public/build --public-path=/build/ --loader:.svg=file --loader:.woff2=file --loader:.mp3=file"
  },
  "devDependencies": {
    "elm-tooling": "1.10.0",
    "elm-watch": "file:../build",
    "esbuild": "0.15.12",
    "run-pty": "4.0.2",
    "uglify-js": "3.17.4"
  }
}